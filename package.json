{
  "name": "@mipha/react",
  "version": "1.0.0",
  "description": "React Component for Mipha",
  "scripts": {
    "test": "sudoo-build --type typescript-react test",
    "build": "sudoo-build --type typescript-react build",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiphaIO/Mipha-React.git"
  },
  "keywords": [
    "Robot",
    "React"
  ],
  "author": "WMXPY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MiphaIO/Mipha-React/issues"
  },
  "homepage": "https://mipha.io",
  "dependencies": {
    "@sudoo/jss": "^1.7.1"
  },
  "devDependencies": {
    "@sudoo/react-force": "^1.10.1",
    "@sudoo/react-force-dev": "^1.10.1",
    "@sudoo/storybook-config": "^1.0.0",
    "@sudoo/storybook-helper": "^1.1.0",
    "@sudoo/triforce": "^3.9.1",
    "@sudoo/triforce-storybook": "^1.2.0",
    "ts-node": "^10.9.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "instrument": true,
    "reporter": [
      "json",
      "text-summary"
    ],
    "report-dir": "coverage",
    "sourceMap": true
  }
}
